
<div class="row-fluid vzs-gallery-content">
  <ul class="thumbnails">
    <%= render @gallery.photos %>
  </ul>
</div>
<% if can? :create, @photo %>
  <div class="row-fluid" id="vzs-new-photo">
    <% @photo ||= current_user.photos.new :gallery_id => @gallery.id if current_user %>
    <h4>Nov√° fotka:</h4>
    <%= render 'photos/form', :photo => @photo %>
  </div>
<% end %>
